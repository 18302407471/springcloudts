<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usercenter.mapper.TsResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.usercenter.entity.TsResource">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="code" property="code" />
        <result column="parent_id" property="parentId" />
        <result column="path" property="path" />
        <result column="is_menu" property="isMenu" />
        <result column="create_time" property="createTime" />
        <result column="oper_username" property="operUsername" />
        <result column="icon_cls" property="iconCls" />
        <result column="action" property="action" />
        <result column="order_num" property="orderNum" />
        <result column="btn_function" property="btnFunction" />
        <result column="btn_title" property="btnTitle" />
        <result column="btn_class" property="btnClass" />
        <result column="note" property="note" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, code, parent_id, path, is_menu, create_time, oper_username, icon_cls, action, order_num, btn_function, btn_title, btn_class, note
    </sql>

</mapper>
